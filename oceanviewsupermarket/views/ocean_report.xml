<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- QWeb Reports -->
        <record id="action_goods_report" model="ir.actions.report">
            <field name="name">Goods</field>
            <field name="model">goods.goods</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">oceanviewsupermarket.report_goods</field>
            <field name="report_file">oceanviewsupermarket.report_goods</field>
            <field name="binding_model_id" ref="model_goods_goods"/>
            <field name="binding_type">report</field>
        </record>

         <record id="action_report_worker" model="ir.actions.report">
            <field name="name">Worker</field>
            <field name="model">worker.worker</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">oceanviewsupermarket.report_worker</field>
            <field name="report_file">oceanviewsupermarket.report_worker</field>
            <field name="binding_model_id" ref="model_worker_worker"/>
            <field name="binding_type">report</field>
        </record>

         <record id="action_report_accounting" model="ir.actions.report">
            <field name="name">Accounting</field>
            <field name="model">accounting.accounting</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">oceanviewsupermarket.report_account</field>
            <field name="report_file">oceanviewsupermarket.report_account</field>
            <field name="binding_model_id" ref="model_accounting_accounting"/>
            <field name="binding_type">report</field>
        </record>

    </data>
</odoo>

